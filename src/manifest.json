{
    "manifest_version": 2,
    "name": "Speed Controller",
    "version": "1.0",

    "description": "Adds a popup that lets you slide a speed slider that affects all video elements on page",

    "icons": {
        "48": "icon.svg",
        "96": "icon.svg"
    },
    "permissions": ["activeTab", "storage"],
    "browser_action": {
        "default_icon": "icon.svg",
        "default_title": "Speed Controller",
        "default_popup": "index.html",
        "browser_style": true
    }
}